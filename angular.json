{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "veritotattoo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/veritotattoo",
      "sourceRoot": "projects/veritotattoo/src",
      "prefix": "vero",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/veritotattoo",
            "index": "projects/veritotattoo/src/index.html",
            "main": "projects/veritotattoo/src/main.ts",
            "polyfills": "projects/veritotattoo/src/polyfills.ts",
            "tsConfig": "projects/veritotattoo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/veritotattoo/src/favicon.ico",
              "projects/veritotattoo/src/assets"
            ],
            "styles": [
              "projects/veritotattoo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/veritotattoo/src/environments/environment.ts",
                  "with": "projects/veritotattoo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "veritotattoo:build:production"
            },
            "development": {
              "browserTarget": "veritotattoo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "veritotattoo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/veritotattoo/src/test.ts",
            "polyfills": "projects/veritotattoo/src/polyfills.ts",
            "tsConfig": "projects/veritotattoo/tsconfig.spec.json",
            "karmaConfig": "projects/veritotattoo/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/veritotattoo/src/favicon.ico",
              "projects/veritotattoo/src/assets"
            ],
            "styles": [
              "projects/veritotattoo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }},
  "cli": {
    "analytics": "a1dcfbfd-0e2f-4b90-9137-f0c9a322b25e"
  },
  "defaultProject": "veritotattoo"
}
