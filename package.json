{
  "name": "moyney",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:moyney": "ng b --prod --baseHref=https://moyney.web.app/ --outputPath=./dist/ && firebase deploy",
    "deploy:moy-call-analyzer": "ng b --project callAnalyzer --prod --baseHref=https://moy-call-analyzer.web.app/ --outputPath=./dist/ && firebase -P moy-call-analyzer deploy --only hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.0.14",
    "@angular/compiler": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "^10.0.14",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "^10.0.14",
    "@angular/router": "^10.0.14",
    "@angular/service-worker": "^10.0.14",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.1",
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/node": "^14.14.14",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "prettier": "^2.2.1",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "jsxBracketSameLine": true,
    "endOfLine": "auto"
  }
}
